{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "minify": true,
          "transpileTemplateLiterals": true,
          "pure": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      [
        "styled-components",
        {
            "ssr": true,
            "displayName": true
        },
        "styled-components"
      ]
    ]
   }